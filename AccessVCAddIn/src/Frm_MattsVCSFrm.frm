Private Sub Form_Load()
Dim sourceFolderStr As String

SetLogWatchingListBox Me.C_LogListBox
ClearLog

'MsgBox "Listing Tables, CurrentDB: " & Application.CurrentDb.Name
Set Forms("MattsVCSFrm").TableSubFrmCtl.Form.Recordset = Nothing
Set Me.TableSubFrmCtl.Form.Recordset = Nothing
ListTables
Set Me.TableSubFrmCtl.Form.Recordset = CodeDb.OpenRecordset("SELECT * FROM " & TABLE_LIST_TABLENAME, dbOpenDynaset)

Me.C_ObjectListLbl.Caption = CountAllDBObjectsGFn(CurrentDb)
CheckForOleFields
CheckForEmbeddedImages
Me.C_CurrentDbNxt = CurrentDb.Name
sourceFolderStr = GetDBFolderNameGFn(CurrentDb) & "\" & GetFSO.GetBaseName(CurrentDb.Name) & "\src"
If CheckAndBuildFolderGFn(sourceFolderStr) Then
    Me.C_SourceDirNxt = sourceFolderStr
Else
    Me.C_SourceDirNxt = "Source folder does not exist and could not be created"
End If

Me.TableSubFrmCtl.Form.Recordset.Requery

End Sub